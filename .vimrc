set nu
set autoindent
set shiftwidth=4
set tabstop=4
set expandtab
colorscheme elflord
filetype on
filetype plugin on
filetype indent on

set paste
syntax on
set fileencodings=utf-8,gb18030,gbk,gb2312,big5
set nocompatible
set vb t_vb=
set autowriteall
set list
set listchars=tab:\|\ ,trail:.,extends:>,precedes:<
set wrap

set guifont=consolas:h12:w7
map \w <ESC>:w<ENTER>
map ,, <ESC>$
map \q <ESC>:q<ENTER>

map \a :tabn<ENTER>
map \s :tabp<ENTER>
map \z :tabe<ENTER>kkk
